{
  "name": "venue-concierge",
  "version": "1.0.0",
  "description": "Event venue discovery platform with AI assistant",
  "main": "server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "tsx watch server/index.ts",
    "client:dev": "vite",
    "build": "npm run build:client && npm run build:server && npm run copy:data",
    "build:client": "vite build",
    "build:server": "tsc -p server/tsconfig.json",
    "copy:data": "cp -r data dist/ && cp -r ads dist/ && cp -r public dist/ && cp -r server/db/*.sql dist/server/db/",
    "start": "node dist/server/index.js",
    "test": "vitest",
    "test:run": "vitest run",
    "db:migrate": "tsx server/scripts/migrate.ts",
    "db:reset": "tsx server/scripts/migrate.ts --reset",
    "db:seed": "tsx server/scripts/migrate.ts --seed-only",
    "db:status": "tsx server/scripts/db-status.ts",
    "db:test": "tsx server/scripts/test-connection.ts"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.57.4",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/node-fetch": "^2.6.13",
    "@types/pg": "^8.10.7",
    "@types/uuid": "^10.0.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.18",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "lucide-react": "^0.344.0",
    "node-fetch": "^3.3.2",
    "pg": "^8.11.3",
    "postcss": "^8.4.35",
    "postgres": "^3.4.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.20.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.5.3",
    "uuid": "^13.0.0",
    "vite": "^5.4.2"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "concurrently": "^8.2.2",
    "supertest": "^6.3.3",
    "tsx": "^4.6.2",
    "vitest": "^1.0.4"
  }
}
